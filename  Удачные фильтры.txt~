    pixels[i]     = (pixels[i]*0)+(pixels[i+1]*0)+(pixels[i+2]*0)+(pixels[i+4]*-2)+(pixels[i+5]*3)+(pixels[i+4]*1)+
    (pixels[i+8]*1.5)+(pixels[i+9]*-2.6)+(pixels[i+10]*-2);// чернобелая фотка

    pixels[i]     = (pixels[i]*0)+(pixels[i+1]*1)+(pixels[i+2]*0)+(pixels[i+4]*-2)+(pixels[i+5]*3)+(pixels[i+4]*1)+
    (pixels[i+8]*0)+(pixels[i+9]*-1.5)+(pixels[i+10]*2);// няшно-голубенький

    pixels[i]     = (pixels[i]*1)+(pixels[i+1]*0)+(pixels[i+2]*-5)+(pixels[i+4]*-2)+(pixels[i+5]*3)+(pixels[i+4]*1.4)+
    (pixels[i+8]*0)+(pixels[i+9]*-2.5)+(pixels[i+10]*2);// черно-белая помятая фотка

    pixels[i]     = (pixels[i]*-0.25)+(pixels[i+1]*2)+(pixels[i+2]*-2)+(pixels[i+4]*-2)+(pixels[i+5]*3)+(pixels[i+4]*3.7)+
    (pixels[i+8]*2)+(pixels[i+9]*-2.5)+(pixels[i+10]*-0.3);// пиздато-розовый

    pixels[i]     = (pixels[i]*-0.25)+(pixels[i+1]*2)+(pixels[i+2]*-2)+(pixels[i+4]*-2)+(pixels[i+5]*3)+(pixels[i+4]*-0.15)+
    (pixels[i+8]*2)+(pixels[i+9]*-2.5)+(pixels[i+10]*-0.3);// почти негатив
